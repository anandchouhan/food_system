<!DOCTYPE html>
<html>
  <head>
    <title>FoodCenter</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if owner_signed_in? %>
      <%= render 'layouts/header' %><br><br><br>
    <% end %>
    <div class="container">
      <%= yield %>
    </div>
  </body>
</html>
